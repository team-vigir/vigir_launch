<?xml version="1.0"?>

<launch>

<!-- Start the Grasp controllers -->
<include file="$(find vigir_onboard)/launch/grasp_controllers.launch"/>

<include file="$(find vigir_onboard)/launch/moveit_planning.launch" />

<!-- Start the default vrc footstep planner setup -->
<include file="$(find flor_footstep_planner)/launch/flor_footstep_planner_vrc_default.launch" />

<!-- Start the default vrc walk monitor -->
<include file="$(find flor_walk_monitor)/launch/walk_monitor.launch" />

<!--  Start the behavior engine -->
<include file="$(find florbe_onboard)/launch/behavior_onboard.launch" />

<!--  Start motion server  -->
<node name="motion_service" pkg="flor_motion" type="motion_service" respawn="true"/>

</launch>

