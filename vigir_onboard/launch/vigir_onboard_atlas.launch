<?xml version="1.0"?>
<launch>
  <!-- Robot Description -->
  <param name="robot_description" textfile="$(find flor_atlas_description)/robots/flor_atlas_v3.urdf"/>
  <param name="use_sim_time" value="false" /> <!-- required = false for hardware -->

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" respawn="true">
    <remap from="joint_states" to="/atlas/joint_states" />
  </node>

  <include file="$(find flor_robot_controller)/launch/flor_robot_controller.launch" />


</launch>
