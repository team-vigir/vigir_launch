<?xml version="1.0"?>

<launch>

    <rosparam file = "$(find vigir_template_library)/object_templates/object_templates.xml"
              ns   = "ot_library"/>

    <rosparam file = "$(find vigir_template_library)/robot_poses/$(optenv FLOR_ROBOT_TYPE atlas_v5)_stand_poses.xml"
              ns   = "stand_poses_library"/>

    <rosparam file = "$(find vigir_template_library)/grasp_templates/$(optenv FLOR_RIGHT_HAND_TYPE r_stump)_grasp_library.xml"
              ns   = "r_hand_library"/>

    <rosparam file = "$(find vigir_template_library)/grasp_templates/$(optenv FLOR_LEFT_HAND_TYPE l_stump)_grasp_library.xml"
              ns   = "l_hand_library"/>

    <include  file = "$(find vigir_manipulation_controller)/launch/$(optenv FLOR_LEFT_HAND_TYPE l_stump)_manipulation_control.launch" />
    <include  file = "$(find vigir_manipulation_controller)/launch/$(optenv FLOR_RIGHT_HAND_TYPE r_stump)_manipulation_control.launch"/>

    <rosparam file="$(find vigir_ocs)/config/hand_parameters.yaml" />

</launch>

