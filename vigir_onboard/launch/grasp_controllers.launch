          ns   = "stand_poses_library"/>

<rosparam file = "$(find vigir_template_library)/grasp_templates/$(env FLOR_RIGHT_HAND_TYPE)_grasp_library.xml"
          ns   = "r_hand_library"/>

<rosparam file = "$(find vigir_template_library)/grasp_templates/$(env FLOR_LEFT_HAND_TYPE)_grasp_library.xml"
          ns   = "l_hand_library"/>

<include  file = "$(find vigir_manipulation_controller)/launch/$(env FLOR_LEFT_HAND_TYPE)_manipulation_control.launch" />
<include  file = "$(find vigir_manipulation_controller)/launch/$(env FLOR_RIGHT_HAND_TYPE)_manipulation_control.launch"/>

</launch>

