    <remap from="camera_out_video" to="lhr_image_cropped_video" />    
  </node>
  
  <!--Right hand-LEFT CAMERA-->
  <!--Add nodelet intended for full image decimation-->
  <node pkg="nodelet" type="nodelet" name="rhl_image_full_decimate" respawn="true"
        args="load vigir_crop_decimate_nodelet/CropDecimateNodelet crop_decimate_nodelet_manager">
    <remap from="camera" to="sandia_hands/r_hand/left/" />
    <remap from="camera_out" to="rhl_image_full" />
    <remap from="camera_out_still" to="rhl_image_full_still" />
    <remap from="camera_out_video" to="rhl_image_full_video" />
  </node>
  <!--Add nodelet intended for region of interest image crop/decimation-->
  <node pkg="nodelet" type="nodelet" name="rhl_image_crop_decimate" respawn="true"
        args="load vigir_crop_decimate_nodelet/CropDecimateNodelet crop_decimate_nodelet_manager">
    <remap from="camera" to="sandia_hands/r_hand/left/" />
    <remap from="camera_out" to="rhl_image_cropped" />
