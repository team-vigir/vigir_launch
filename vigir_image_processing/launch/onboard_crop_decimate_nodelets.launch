    <remap from="camera_out_video" to="rhl_image_cropped_video" />
  </node>

  <!--Right hand-RIGHT CAMERA-->
  <!--Add nodelet intended for full image decimation-->
  <node pkg="nodelet" type="nodelet" name="rhr_image_full_decimate" respawn="true"
        args="load vigir_crop_decimate_nodelet/CropDecimateNodelet crop_decimate_nodelet_manager" output="screen">
    <remap from="camera" to="sandia_hands/r_hand/right/" />
    <remap from="camera_out" to="rhr_image_full" />
    <remap from="camera_out_still" to="rhr_image_full_still" />
    <remap from="camera_out_video" to="rhr_image_full_video" />
  </node>
  <!--Add nodelet intended for region of interest image crop/decimation-->
  <node pkg="nodelet" type="nodelet" name="rhr_image_crop_decimate" respawn="true"
        args="load vigir_crop_decimate_nodelet/CropDecimateNodelet crop_decimate_nodelet_manager" output="screen">
    <remap from="camera" to="sandia_hands/r_hand/right/" />
    <remap from="camera_out" to="rhr_image_cropped" />
    <remap from="camera_out_still" to="rhr_image_cropped_still" />
    <remap from="camera_out_video" to="rhr_image_cropped_video" />
  </node>
   
</launch>

