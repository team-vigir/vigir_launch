  </node>
  <!--Add nodelet intended for region of interest image crop/decimation-->
  <node pkg="nodelet" type="nodelet" name="lhl_image_crop_decimate" respawn="true"
        args="load vigir_crop_decimate_nodelet/CropDecimateNodelet crop_decimate_nodelet_manager">
    <remap from="camera" to="sandia_hands/l_hand/left/" />
    <remap from="camera_out" to="lhl_image_cropped" />
    <remap from="camera_out_still" to="lhl_image_cropped_still" />
    <remap from="camera_out_video" to="lhl_image_cropped_video" />   
  </node>

  <!--Left hand-RIGHT CAMERA-->
  <!--Add nodelet intended for full image decimation-->
  <node pkg="nodelet" type="nodelet" name="lhr_image_full_decimate" respawn="true"
        args="load vigir_crop_decimate_nodelet/CropDecimateNodelet crop_decimate_nodelet_manager">
    <remap from="camera" to="sandia_hands/l_hand/right/" />
    <remap from="camera_out" to="lhr_image_full" />
    <remap from="camera_out_still" to="lhr_image_full_still" />
    <remap from="camera_out_video" to="lhr_image_full_video" /> 
  </node>
  <!--Add nodelet intended for region of interest image crop/decimation-->
  <node pkg="nodelet" type="nodelet" name="lhr_image_crop_decimate" respawn="true"
